<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nonggun.mediq">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controllers.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controllers.register.RegisterProfileActivity" />
        <activity android:name=".controllers.register.RegisterNameActivity" />
        <activity android:name=".controllers.register.RegisterPasswordActivity" />
        <activity
            android:name=".controllers.queue.QueueActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".controllers.register.FinishRegisterActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".controllers.queue.InQueueActivity"
            android:launchMode="singleTask" />
        <activity android:name=".controllers.profile.ProfileActivity" />
    </application>

</manifest>